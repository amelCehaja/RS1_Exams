@model RS1_Ispit_2017_06_21_v1.ViewModels.UrediMaturskiStavkaVM

<h3>Evidentiranje rezultata</h3>
<form id="forma">
    <input asp-for="ID" hidden />
    <div class="form-group">
        <label>Učenik:</label>
        <p>@Model.ImePrezime</p>
    </div>
    <div class="form-group">
        <label>Bodovi:</label>
        <input asp-for="Bodovi" class="form-control"/>
    </div>
    <button type="submit" class="btn btn-success">Spremi</button>
</form>
<script src="~/js/MaturskiStavka.js"></script>